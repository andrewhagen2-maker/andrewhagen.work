{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "dashboard.andrewhagen.work"
        }
      ],
      "destination": "https://dashboard-ra4y8jaxetweo6u9gdqg86.streamlit.app",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "game.andrewhagen.work"
        }
      ],
      "destination": "/game/$1"
    }
  ],
  "headers": [
    {
      "source": "/game/(.*)",
      "headers": [
        { "key": "Cross-Origin-Opener-Policy",   "value": "same-origin" },
        { "key": "Cross-Origin-Embedder-Policy",  "value": "credentialless" }
      ]
    }
  ]
}
